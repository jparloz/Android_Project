<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval">
        <solid
            android:color="@color/white"/>
        <size
            android:width="4dp"
            android:height="4dp"/>
        <stroke
            android:width="1dp"
            android:color="@color/night"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/baseline_nightlight_round_24"
        android:top="4dp"
        android:bottom="4dp"
        android:right="4dp"
        android:left="4dp">
    </item>

</layer-list>